{
  "name": "violentmonkey-mx",
  "version": "2.8.0",
  "description": "Violentmonkey for Maxthon",
  "scripts": {
    "dev": "gulp watch",
    "prebuild": "npm run lint && gulp clean",
    "build": "cross-env NODE_ENV=production gulp build",
    "analyze": "webpack --profile --json --config scripts/webpack.conf.js | webpack-bundle-size-analyzer",
    "analyze:json": "webpack --profile --json --config scripts/webpack.conf.js > stats.json",
    "i18n": "gulp i18n",
    "lint": "eslint --ext .js,.vue .",
    "svgo": "svgo --config .svgo.yml icons",
    "prepush": "npm run lint"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.1",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.8.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "gulp": "^3.9.1",
    "gulp-filter": "^5.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-svg-sprite": "^1.3.7",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "html-webpack-plugin": "^2.29.0",
    "postcss-loader": "^2.0.8",
    "postcss-scss": "^1.0.2",
    "precss": "^2.0.0",
    "through2": "^2.0.3",
    "vue-loader": "^13.0.4",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.8.1",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "wrapper-webpack-plugin": "^1.0.0"
  },
  "author": "Gerald <i@gerald.top>",
  "repository": {
    "type": "git",
    "url": "https://github.com/violentmonkey/violentmonkey-mx.git"
  },
  "bugs": {
    "url": "https://github.com/violentmonkey/violentmonkey-mx/issues"
  },
  "homepage": "https://github.com/violentmonkey/violentmonkey-mx",
  "license": "MIT",
  "dependencies": {
    "codemirror": "^5.30.0",
    "core-js": "^2.4.1",
    "tldjs": "^2.2.0",
    "vue": "^2.4.4",
    "vueleton": "^0.5.0"
  },
  "engines": {
    "node": ">=8"
  }
}
